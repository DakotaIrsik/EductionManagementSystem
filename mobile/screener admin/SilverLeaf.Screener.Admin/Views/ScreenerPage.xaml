<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SilverLeaf.Screener.Admin.Views.ScreenerPage"
             Title="{Binding Resources[PageTitleScreenerPage]}">
    <ContentPage.ToolbarItems>
        <ToolbarItem
                 IconImageSource="baseline_cloud_download_white.png"
                 Command="{Binding DownloadCommand}"
                 Order="Primary"
                 Priority="0" />
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <ScrollView>
            <Image Source="{Binding ScreenerSummary.Url}" />
        </ScrollView>
    </ContentPage.Content>
</ContentPage>